  "reactSnap": {
    "headless": true,
    "crawl": false,
    "destination": "../templates/prerendered",
    "source": "build",
    "concurrency": 1,
    "minifyHtml": {
      "collapseBooleanAttributes": true,
      "collapseWhitespace": true,
      "decodeEntities": true,
      "keepClosingSlash": true,
      "sortAttributes": true,
      "sortClassName": false
    },
    "viewport": {
      "height": 400,
      "width": 730
    },
    "include": [
      "",
      "/blog"
    ],
    "publicPath": "/"
  }
