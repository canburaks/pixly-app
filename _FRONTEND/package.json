  "license": "ISC",

  "scripts": {
    "start": "webpack-dev-server --open --hot --mode development",
    "build": "webpack --mode production",
    "deploy-frontend": "python config/upload.py",
    "deploy-backend": "cd .. && git add . && git commit -m '...' && git push",
    "snap": "react-snap",
    "fullbuild": "npm run build && npm run deploy-frontend && npm run snap && npm run deploy-backend",
    "snapshot": "snapshotify"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "debug": true
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-property-mutators",
      "@babel/plugin-transform-template-literals",
      "@babel/plugin-transform-literals",
      "@babel/plugin-transform-spread",
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/core",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "core"
      ],
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/icons",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "icons"
      ]
    ]
  },
  "browserslist": [
    "> 0.01%",
    "safari > 4",
    "android > 3",
    "and_chr >= 61",
    "and_uc >= 11.4",
    "and_qq >= 1.2",
    "edge >= 16",
    "and_ff >= 56",
    "chrome > 37",
    "firefox >= 43",
    "opera > 48",
    "samsung >= 4",
    "baidu >= 7.12",
    "bb >= 10",
    "ie >= 8",
    "ios_saf >= 8",
    "last 20 versions"
  ],
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "overrideBrowserslist": [
          "safari > 5",
          "safari > 4",
          "android > 3",
          "and_chr >= 61",
          "and_uc >= 11.4",
          "and_qq >= 1.2",
          "edge >= 16",
          "and_ff >= 56",
          "chrome > 37",
          "firefox >= 43",
          "opera > 48",
          "samsung >= 4",
          "baidu >= 7.12",
          "bb >= 10",
          "ie >= 8",
          "ios_saf >= 8"
        ],
        "autoprefixer": {
          "grid": true
        }
      }
    }
  },
